            <div class="column7 setlocation parbase">



<a tabindex="1" id="location_tab" class="menu_link" href="#" title="Set Location"> 
                <span> 
                    <div class="loc_nat">
                        <span>Set </span>Location
                        <img src="/etc/designs/visas/images/person_graphic.png" alt="set location icon"/>
                    </div>
                    <div class="loc_nat">
                        <span>Set </span>Nationality
                        <img src="/etc/designs/visas/images/person_graphic.png" alt="set nationality icon"/>
                    </div>
                </span> 
            </a>
            <div class="location_slide_panel">
                <p>Embassy: <span class="embassy_location yellow_text">Not set</span></p>
                <p>Nationality: <span class="nationality_location yellow_text">Not set</span></p>
                <a class="edit_location_link" href="#" title="Edit Location"><span>Edit Location</span></a> </div>
            <div id="set_location_panel" class="dropdown_2columns blue">
                <div class="col_2 no_border">
                    <p class="form_caption">Create a profile to customize visa  information for temporary visitors to the United States.</p>
                    <!--<p class="helper_text">Based on your location, we think the closest embassy to you is in <span class="embassy_location yellow_text">Tokyo</span></p>-->
                    
                    <div class="SetLocation_Autocomplete autocomplete_setlocation parbase">

  

<form>
    <p>
        <div class="autocompleteWrapper">
            <div id="Autocomplete_SetLocation_Post_container">
                <label for="Autocomplete_SetLocation_Post_input">Where will you apply for a nonimmigrant visa?</label>
                <span class="helper_text"></span>
                <input tabindex="-1" type="text" name="Autocomplete_SetLocation_Post_input" id="Autocomplete_SetLocation_Post_input" class="autocomplete_input input_helper_text post" title="Enter a U.S. Embassy or Consulate" />
            </div>
        </div>
        
        <!--<a class="yellow_text" href="http://www.usembassy.gov/" title="U.S. Embassy/Consulate website">Go to U.S. embassy/consulate website</a>-->
    </p>
    <p>
        <div class="autocompleteWrapper">
            <div id="Autocomplete_SetLocation_Nationality_container">
                <label for="Autocomplete_SetLocation_Nationality_input">What country/authority issued your passport?</label>
                <span class="helper_text"></span>
                <input tabindex="-1" type="text" name="Autocomplete_SetLocation_Nationality_input" id="Autocomplete_SetLocation_Nationality_input" class="autocomplete_input input_helper_text country" title="Enter a country/authority or area" />
            </div>
        </div>
    </p>
    <p class="blue">
        <span class="btn">
            <input tabindex="-1" type="submit"  class="autocomplete_submit" value="SAVE & CLOSE" />
        </span>
        <a tabindex="-1" href="#" class="clear_form">Clear Form</a>
        
    </p>
    <a tabindex="-1" href="#" class="close_panel" title="close panel">Close <span>X</span></a>        
</form>

<script type="text/javascript" src="/etc/designs/travel/TSGglobal_libs/data/PostsVWT.js"></script>
<script type="text/javascript" src="/etc/designs/travel/TSGglobal_libs/data/CountryListSetNationality.js"></script>

<script type="text/javascript">
jQuery(function(){
    /*add hidden field*/
    jQuery('<input type="hidden" class="hidden_code" name="hidden_code" value="">').appendTo('#Autocomplete_SetLocation_Post_container');
    jQuery('<input type="hidden" class="hidden_code" name="hidden_code" value="">').appendTo('#Autocomplete_SetLocation_Nationality_container');
    
    //getting options and instantiating the autocomplete
    var setLocationPostOpts = travelStateGov.globalfx.createAutoOpts("setLocationPost",travelStateGov.PostsVWT.sourceData, "Autocomplete_SetLocation_Post",true,25);   
    var setLocationNationalityOpts = travelStateGov.globalfx.createAutoOpts("setLocationNationality",travelStateGov.CountryListSetNationality.sourceData, "Autocomplete_SetLocation_Nationality",true,25);
    jQuery("#Autocomplete_SetLocation_Post_input").autocomplete(setLocationPostOpts); 
    jQuery("#Autocomplete_SetLocation_Nationality_input").autocomplete(setLocationNationalityOpts);

  //function to populate selected answer for selected post
    var showSelectedPost = function () {
        var postCookieValue = travelStateGov.globalfx.getTsgStateGovCookieValue("post"),
            postCookieCode = travelStateGov.globalfx.getTsgStateGovCookieCode("post");
               
        jQuery('#tab_7_li .location_slide_panel .embassy_location').html(postCookieValue);
        //hide "Set" word
        jQuery('.loc_nat').eq(0).find('span').hide();
        if(postCookieValue.length > 0) {
            var locationTabImgWrap = jQuery('#tab_7_li #location_tab .loc_nat'),
                imagesPath = "/etc/designs/visas/images/",
                thisHiddenCodeInput = jQuery('#Autocomplete_SetLocation_Post_container input.hidden_code');
            
            locationTabImgWrap.eq(0).find('img').prop('src', imagesPath + "person_graphic_green.png").end().addClass("active_state post");
            thisHiddenCodeInput.val(postCookieCode);
        } else {
            //clean up form
            var locationTabImgWrap = jQuery('#tab_7_li #location_tab .loc_nat'),
                imagesPath = "/etc/designs/visas/images/",
                thisHiddenCodeInput = jQuery('#Autocomplete_SetLocation_Post_container input.hidden_code'),
                thisInputField = jQuery('#Autocomplete_SetLocation_Post_container input.autocomplete_input '),
                thisInputTitle = thisInputField.prop("title");
                
            locationTabImgWrap.eq(0).find('img').prop('src', imagesPath + "person_graphic.png").end()
                .removeClass("active_state post").find("span").show();
            thisHiddenCodeInput.val("");
            thisInputField.val(thisInputTitle);
                
        }   
    };
    //function to populate selected answer for selected post
    var showSelectedNationality = function () {
        var nationalityCookieValue = travelStateGov.globalfx.getTsgStateGovCookieValue("nationality"),
            nationalityCookieCode = travelStateGov.globalfx.getTsgStateGovCookieCode("nationality");
        jQuery('#tab_7_li .location_slide_panel .nationality_location').html(nationalityCookieValue);
        //hide "Set" word
        jQuery('.loc_nat').eq(1).find('span').hide();
        //show flag
        if(nationalityCookieCode.length > 0){
            var locationTabImgWrap = jQuery('#tab_7_li #location_tab .loc_nat'),
                imagesPath = "/content/dam/travel/flags_small/",
                thisHiddenCodeInput = jQuery('#Autocomplete_SetLocation_Nationality_container input.hidden_code');
            
            locationTabImgWrap.eq(1).find('img').prop('src', imagesPath + nationalityCookieCode.toLowerCase() + ".png").end().addClass("active_state nat");
            thisHiddenCodeInput.val(nationalityCookieCode);
        }else{
            var locationTabImgWrap = jQuery('#tab_7_li #location_tab .loc_nat'),
                imagesPath = "/etc/designs/visas/images/",
                thisHiddenCodeInput = jQuery('#Autocomplete_SetLocation_Nationality_container input.hidden_code'),
                thisInputField = jQuery('#Autocomplete_SetLocation_Nationality_container input.autocomplete_input'),
                thisInputTitle = thisInputField.prop("title");
            
            locationTabImgWrap.eq(1).find('img').prop('src', imagesPath + "person_graphic.png").end()
                .removeClass("active_state nat").find("span").show();
            thisHiddenCodeInput.val("");
            thisInputField.val(thisInputTitle);
        }           
    };
    //register callback
    travelStateGov.globalfx.registerCallback(showSelectedPost);
    travelStateGov.globalfx.registerCallback(showSelectedNationality);
    
    //Check the cookie
    if(travelStateGov.globalfx.getTsgStateGovCookie("post")){
        showSelectedPost(); 
    }else{
        //console.log("cookie not set");
    }
    if(travelStateGov.globalfx.getTsgStateGovCookie("nationality")){
        showSelectedNationality();
    }else{
        //console.log("cookie not set");
    }       

    //Set the cookie on submit
    jQuery("#set_location_panel form .autocomplete_submit").click(function(e){
        e.preventDefault();
        travelStateGov.setLocation = true;
        //one submit button for two autocompletes
        var thisSubmitBtn = jQuery(this),
        wrappedInputs = thisSubmitBtn.parents('#set_location_panel form').find(".autocomplete_input"),
        wrappedHiddenCodes = thisSubmitBtn.parents('#set_location_panel form').find(".hidden_code"),
        wrappedPostInputs = jQuery(".post");
        wrappedCountryInputs = jQuery(".country");
        inputTermPost = jQuery.trim(wrappedInputs.eq(0).val()),
        inputTermNationality = wrappedInputs.eq(1).val(),
        inputCodePost = jQuery.trim(wrappedHiddenCodes.eq(0).val()),
        inputCodeNationality = jQuery.trim(wrappedHiddenCodes.eq(1).val()),
        cookieStringPosts = "",
        cookieStringNationality = "";
        
        /*******************************
            handle posts input
        *******************************/
    if( inputTermPost.length > 0 && inputTermPost != wrappedInputs.eq(0).attr("title") && jQuery.trim(inputCodePost) ) { 
        if(travelStateGov.globalfx.isInDataSource(inputTermPost, travelStateGov.PostsVWT.sourceData)){
            
            if(inputTermPost.length > 0 && inputTermPost != wrappedInputs.eq(0).prop("title")){
                //update cookie with embassy
                travelStateGov.globalfx.setTsgStateGovCookie("post", inputCodePost+":"+inputTermPost);
                
                //update embassy slide tab with embassy
                travelStateGov.globalfx.triggerRegisteredCallback();
            }else{
                //console.log("Post input was empty");
            }
        }else{
            alert("Please select a valid city.");
            //wrappedInputs.eq(0).val("").focus();
        }
    }
    
    if(inputTermNationality.length > 0 && inputTermNationality != wrappedInputs.eq(1).attr("title")) {
    
        if(travelStateGov.globalfx.isInDataSource(inputTermNationality, travelStateGov.CountryListSetNationality.sourceData)){  
            /*******************************
                handle nationality input
            *******************************/
            if(inputTermNationality.length > 0 && inputTermNationality != wrappedInputs.eq(1).prop("title")){
                //update cookie with embassy
                travelStateGov.globalfx.setTsgStateGovCookie("nationality", inputCodeNationality+":"+inputTermNationality);
                
                //update embassy slide tab with embassy
                travelStateGov.globalfx.triggerRegisteredCallback();               
            }else{
                //console.log("Nationality input was empty");
            }
        }else{
            alert("Please select a valid country.");
            //wrappedInputs.eq(1).val("").focus();
        }
    
    }
    //finally close panel when all is done
    jQuery("#location_tab").trigger('click');
   

    jQuery("#Autocomplete_SetLocation_Post_input").removeAttr("tabindex");
    jQuery("#Autocomplete_SetLocation_Nationality_input").removeAttr("tabindex");
    jQuery(".autocomplete_submit").removeAttr("tabindex");
    jQuery(".clear_form").removeAttr("tabindex");
    jQuery(".close_panel").removeAttr("tabindex");
    
    jQuery("#location_tab").focus();
    jQuery("#tab_7_li").css("outline","1px solid #999");
                
});

jQuery('#set_location_panel form').submit(function(e){
    e.preventDefault();
        
    jQuery('#set_location_panel form .autocomplete_submit').trigger('click');
});
//handler for clear form link
jQuery("a.clear_form", "#set_location_panel").click(function(e){
    e.preventDefault();
    //clear cookies
    var deleteResponsePost = jQuery.removeCookie("visasStateGovPost",{path: '/'});
    var deleteResponseNat = jQuery.removeCookie("visasStateGovNationality",{path: '/'});
    
    //trigger the registered call back function
    //clears other form fields
    travelStateGov.globalfx.triggerRegisteredCallback();
}); 

//handler for close anchor
jQuery('#set_location_panel form .close_panel').click(function(e){
    e.preventDefault();
    //close set location panel
      
    $("#Autocomplete_SetLocation_Post_input").removeAttr("tabindex");
    $("#Autocomplete_SetLocation_Nationality_input").removeAttr("tabindex");
    $(".autocomplete_submit").removeAttr("tabindex");
    $(".clear_form").removeAttr("tabindex");
    $(".close_panel").removeAttr("tabindex");
  
    jQuery("#location_tab").trigger('click');
    
    jQuery("#location_tab").focus();
    jQuery("#tab_7_li").css("outline","1px solid #999");
});
//handler for edit location in the slide down menu
jQuery('#tab_7_li .location_slide_panel .edit_location_link').click(function(e){
    e.preventDefault();
    //console.log("click");
    //trigger location panel to open
    
        $("#Autocomplete_SetLocation_Post_input").attr('tabindex', 1);
        $("#Autocomplete_SetLocation_Nationality_input").attr('tabindex', 1);
        $(".autocomplete_submit").attr('tabindex', 1);
        $(".clear_form").attr('tabindex', 1);
        $(".close_panel").attr('tabindex', 1);
        
    jQuery("#location_tab").trigger('click');
    jQuery(".autocomplete_input.input_helper_text.post.ui-autocomplete-input").focus();
});

jQuery('#tab_7_li .location_slide_panel .edit_location_link').keydown(function(e){
    e.preventDefault();
    //console.log("text");
    //trigger location panel to open
    if(e.keyCode === 13 || e.keyCode === 32){
    	jQuery("#tab_7_li .location_slide_panel .edit_location_link").trigger('click');
    }
});

    jQuery("#Autocomplete_SetLocation_Nationality_input").keydown(function(e){
        if(e.keyCode === 13){
            jQuery("#set_location_panel .autocomplete_submit").focus();
            //console.log("autocomplete_submit");
        }
    });
});

</script>

 </div>


                </div>
            </div></div>

        </li>                   
    </ul>
</div>
</div>

</div>

