        ga("send", "event", "TSG_OCS", countryName + " Expand All", trackURL, 7);
        if(window.location.hostname.indexOf("state.gov") > 0) {
            gas("send", "event", "TSG_OCS", countryName + " Expand All", trackURL, 7);
        };
    });
    jQuery("#country_detail .expandos dt").on("click.expand", function(){
