<script>
    /* START Google Analytics */
    var gaAccount = "UA-21044428-4"; // Production Account
    
    if(window.location.hostname !== "travel.state.gov") {
        gaAccount = "UA-21044428-5"; // QA Author Account
    } 
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    var trackURL = window.location.href;
    
    ga('create', gaAccount, 'auto');
    ga('send', 'pageview');
    
    jQuery("a[href*='://step.state.gov/step'").on("click.stepPromo", function(e) {
        e.preventDefault();
        //var title = document.getElementsByTagName("title")[0].innerHTML;
        ga("send", "event", "TSG_OCS", "STEP-Enroll", trackURL, 1, {
            hitCallback: function() {
                document.location = e.target.href;
            }
        });
        if(window.location.hostname.indexOf("state.gov") > 0) {
            gas("send", "event", "TSG_OCS", "STEP-Enroll", trackURL, 1, {
                hitCallback: function() {
                document.location = e.target.href;
            }
          });
        };
    });
        
    jQuery("#country_detail .expandos .expand_all").on("click.expandAll", function(){
        var countryName = jQuery('#country_header_section h1').text();
        ga("send", "event", "TSG_OCS", countryName + " Expand All", trackURL, 7);
        if(window.location.hostname.indexOf("state.gov") > 0) {
            gas("send", "event", "TSG_OCS", countryName + " Expand All", trackURL, 7);
        };
    });
    jQuery("#country_detail .expandos dt").on("click.expand", function(){
        var countryName = jQuery('#country_header_section h1').text();
        var expandName = jQuery(this).find('span:eq(1)').text();
        ga("send", "event", "TSG_OCS", countryName + " " + expandName, trackURL, 1);
        if(window.location.hostname.indexOf("state.gov") > 0) {
            gas("send", "event", "TSG_OCS", countryName + " " + expandName, trackURL, 1);
        };
    });
    
    /* END Google Analytics */
</script>


            
